CC	Makefile	/^CC = gcc$/;"	m
CFLAGS	Makefile	/^CFLAGS = -I$(LIB_DIR)  # Include the library's header files (if needed)$/;"	m
EXEC	Makefile	/^EXEC = a$/;"	m
LDFLAGS	Makefile	/^LDFLAGS = -L$(LIB_DIR) -lmylib -lpthread -Wl,-rpath,.\/$/;"	m
LIB_DIR	Makefile	/^LIB_DIR = ..\/generate_SO$/;"	m
LIB_NAME	Makefile	/^LIB_NAME = libmylib.so$/;"	m
MYLIB_H	mylib.h	2;"	d
MYLIB_NOTIFIER	mylib.h	/^typedef void (*MYLIB_NOTIFIER)(void *result);$/;"	t
OBJ	Makefile	/^OBJ = $(SRC:.c=.o)$/;"	m
SRC	Makefile	/^SRC = main.c$/;"	m
age	mylib.h	/^    int age;$/;"	m	struct:__anon1
gp_student	main.c	/^student_t* gp_student = NULL;$/;"	v
main	main.c	/^int main() $/;"	f
name	mylib.h	/^    char* name;$/;"	m	struct:__anon1
print_every_second_thread	main.c	/^void* print_every_second_thread(void *arg)$/;"	f
print_student	main.c	/^void print_student(const student_t *student)$/;"	f
sem	main.c	/^sem_t sem;$/;"	v
student_t	mylib.h	/^} student_t;$/;"	t	typeref:struct:__anon1
supervisor_thread	main.c	/^void* supervisor_thread(void* arg) $/;"	f
task_callback	main.c	/^int task_callback(void* result) $/;"	f
